<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="sk.magiksoft.sodalis.core.settings.Settings" table="Settings">
        <id column="id" name="id">
            <generator class="sequence"/>
        </id>
        <property name="creationTime"/>
        <property name="settingsKey"/>
        <property name="userUID"/>
        <property name="values" type="serializable" length="16777216"/>
        <property name="globalValues" type="serializable" length="16777216"/>
    </class>
</hibernate-mapping>
